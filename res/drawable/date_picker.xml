<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        
        <shape>
            
            <solid android:color="#5fff" />
            
            <stroke 
                android:width="1.0sp"
                android:color="#3444" />
            
        </shape>
        
    </item>
	
	<item android:state_pressed="true" >
        
        <shape>
            
            <solid android:color="#f58634" />
            
            <stroke 
                android:width="1.0sp"
                android:color="#3444" />
            
        </shape>
        
    </item>
    
</selector>